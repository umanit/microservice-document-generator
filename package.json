{
  "name": "microservice-document-generator",
  "version": "1.0.0",
  "description": "Microservice document generator is a NodeJS microservice which allows to generate png or PDF file from an URL or a HTML string using Puppeteer.",
  "main": "lib/index.js",
  "dependencies": {
    "connect": "^3.7",
    "core-js": "^3.1",
    "path-is-inside": "^1.0",
    "puppeteer-cluster": "^0.21",
    "puppeteer-core": "^11.0",
    "regenerator-runtime": "^0.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "nodemon": "^2.0"
  },
  "scripts": {
    "start": "node lib/index.js",
    "start-dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "build": "babel src -d lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:umanit/microservice-document-generator.git"
  },
